PROGRAM NXTEST
DEF_FILE_PATH = 'C:\QMACCOUNTS\nxtReportDefs'
DEF_FILE_NAME = "TEST1.DAT"
REPORT_FILE_PATH = "C:\QMACCOUNTS\Reports"
REPORT_FILE_NAME = "MyTestReport.xml"
REPLACE_ARRAY = '' 
OPTIONS = ''
OPTIONS<1> = 'XLSX'
OPTIONS<2> = 'DEBUG'
* OPTIONS<3> = 'xml'
MY_STATUS = 0

CRT 'Here we go'
CRT "DEF_FILE_PATH    :":DEF_FILE_PATH
CRT "DEF_FILE_NAME    :":DEF_FILE_NAME
CRT "REPORT_FILE_PATH :":REPORT_FILE_PATH
CRT "REPORT_FILE_NAME :":REPORT_FILE_NAME
CRT "REPLACE_ARRAY    :":REPLACE_ARRAY
CRT "OPTIONS          :":OPTIONS
CRT "MY_STATUS        :":MY_STATUS
CRT
CALL NXTREPORT(DEF_FILE_PATH,DEF_FILE_NAME,REPORT_FILE_PATH,REPORT_FILE_NAME,REPLACE_ARRAY, OPTIONS, MY_STATUS)

CRT 'And we are back'
CRT "DEF_FILE_PATH    :":DEF_FILE_PATH
CRT "DEF_FILE_NAME    :":DEF_FILE_NAME
CRT "REPORT_FILE_PATH :":REPORT_FILE_PATH
CRT "REPORT_FILE_NAME :":REPORT_FILE_NAME
CRT "REPLACE_ARRAY    :":REPLACE_ARRAY
CRT "OPTIONS          :":OPTIONS
CRT "MY_STATUS        :":MY_STATUS
END
